void reconnect() {
  int counter = 0; // counter for counting restart time
  while (!client.connected()) { // waiting for client connection
    if (counter==5){ // if count value is excided value 5 then restart the esp8266 board
//      ESP.restart(); // function to restart the esp8266 board
      return;
    }
    counter+=1; // incrimenting counter variable by value of 1 
    Serial.print(" message from reconnect():: ");
    Serial.print("Attempting MQTT connection..."); // printing debugging information to the serial monitor
   
    if (client.connect("4143c7ba-a45a-4aef-8efc-df0e5c403d52", "engineer", "ec@ldce")) { // connect the client to the broker by providing client id, user name, password
      Serial.print("connected, rc="); // print "connected" when client is connected to broker for debugging
      Serial.println(client.state()); // client.state() provide diffrent state of rc value 
      subscriber(); // calling the subscriber function to subscribe a perticular topic for message
    } else {
      Serial.print("failed, rc="); // if connection fail the printing debug information with rc value
      Serial.print(client.state()); // client.state() provide diffrent state of rc value
      Serial.println(" try again in 5 seconds"); // printing debug information 
      delay(5000); // waiting for 5 seconds
    }
  }
}

