}

/**************************************************************************************************************/
/********************************************** CALLBACK FUNCTION *********************************************/
/**************************************************************************************************************/

void callback(char* topic, byte* message, unsigned int length) { // function for recived message from MQTT broker
  Serial.print("  message from callback():: ");
  Serial.print("Message arrived ["); // printing debug info to indicate message is arrived from MQTT broker
  Serial.print(topic); // printing topic that we have subscribed 
  Serial.print("] "); // printing debug info close tag 

  /************************************* EXTRACTING / TRANSMITTING MESSAGE ************************************/
  char data[length] = {};
  
  for (int i = 0; i < length; i++) { // loop that is count for recived message length
//    Serial.print((char)message[i]); // printing recived message character to the serial monitor
//    irsend.sendNEC((char)message[i]); // sending recived message character to the ir sensor
    data[i] = message[i];
  }

  unsigned int final_data = strtoul(data, NULL, 16);
  Serial.println(final_data, HEX);
  irsend.sendNEC(final_data);
//  irsend.sendNEC((char)'\n'); // sending new line character to the ir sensor
//  irsend.sendNEC((char)'\r'); // sending carriage return character to the ir sensor
//  Serial.println(); // printing new line for better visulization of debugging information
}

