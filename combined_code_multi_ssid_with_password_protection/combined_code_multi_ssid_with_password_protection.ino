/**************************************************************************************************************/
/******************************************** EEPROM WRITE FUNCTION *******************************************/
/**************************************************************************************************************/

void eepromWrite(int address, String ssid, String pass){
  byte len = ssid.length();
  EEPROM.write(address, len);

  for(int i=0; i<len; i++){
    EEPROM.write(address + 1 + i, ssid[i]);
  }
  EEPROM.commit();

  EEPROM.write(len+1, pass.length());
  len += 1;
  for(int i=0; i<pass.length(); i++){
    EEPROM.write(len + 1 + i, pass[i]);
  }
  EEPROM.commit();

  Serial.print("message from epromWrite():: SSID ");
  Serial.print(ssid);
  Serial.println(" saved successfully in EEPROM");
  Serial.print("message from epromWrite():: Password ");
  Serial.print(pass);
  Serial.println(" saved successfully in EEPROM");
}

/**************************************************************************************************************/
/******************************************** EEPROM READ FUNCTION ********************************************/
/**************************************************************************************************************/

void eepromRead(int address){
  int lens = EEPROM.read(address);
  char datas[lens + 1];

  for(int i=0; i<lens; i++){
    datas[i] = EEPROM.read(address + 1 + i);
  }

  datas[lens] = '\0';
  
  lens = lens + 1;
  int lenp = EEPROM.read(lens);
  char datap[lenp +1];
   
  for(int i=0; i<lenp; i++){
    datap[i] = EEPROM.read(lens + 1 + i);
  }
  
  datap[lenp] = '\0';

  EEPROMssid = String(datas);
  EEPROMpass = String(datap);
}

/**************************************************************************************************************/
/********************************************* WIFI SETUP FUNCTION ********************************************/
/**************************************************************************************************************/

boolean setup_wifi() {
  boolean state = true; // state of wifi is coneected or not
  int i = 0; // counter for waiting time of wifi connection
  
  /********************************************* WIFI CONFIGURATION *******************************************/
  Serial.print("message from setup_wifi():: ");
  Serial.println("Connecting to network...."); // printing to serial monitor for debugging purpose
  ssid = ssid_data[index_value]; // indexing the ssid_data array and passing the value to ssid variable that we declered eariler
  password = pass_data[index_value]; // indexing the pass_data array and passing the value to password variable that we declered eariler
  WiFi.mode(WIFI_STA); // configuring wifi as station mode to connect access point
  
  if(eepromFlag){
    eepromRead(0);
    WiFi.begin(EEPROMssid, EEPROMpass);
  }

  else{
    WiFi.begin(ssid, password); 
  }
  
  /**************************************** WAITING FOR WIFI CONNECTION ***************************************/
  while(WiFi.status() != WL_CONNECTED){ // check if wifi is connected or not
  //    Serial.print("."); // printing ... to the serial monitor indicateing it is witing for wifi
    delay(500); // ideal for 1 second, waiting for wifi
    if (i > 20) { // compairing value of variable i with constant to waiting for wifi
      state = false; // if wifi is connected then state is false
      break; // break loop if wifi is connected
    }
    i++;
  }

  /*************************************** PRINTING WIFI CONNCTION INFO **************************************/
  if(state){
    if(eepromFlag){
      Serial.print("message from setup_wifi():: ");
      Serial.print(EEPROMssid);
    }
    else{
      Serial.print("message from setup_wifi():: ");
      Serial.print(ssid);
    }
    Serial.println("'s WiFi Connected Successfully"); // printing to serial monitor to indicate, wifi is connected
    Serial.print("message from setup_wifi():: ");
    Serial.print("IP Address :"); // printing the "IP Address :" string to the serial monitor
    IPAddress myIP = WiFi.localIP(); // getting local ip of the esp8266 board by calling WiFi.localIP() function from ESP8266WiFi.h library
    Serial.println(myIP); // print local ip that we get erlier in above line
  }
  return state; // returning wifi state value
}

